{
  "expo": {
    "name": "DJR Group",
    "slug": "djrgroup",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#283673"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#283673"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#283673",
        "backgroundImage": "./assets/icon.png"
      },
      "package": "com.sporti.djrgroup",
      "permissions": []
    },
    "ios": {
      "buildNumber": "3",
      "bundleIdentifier": "com.sporti.djrgroup",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to collect the user's profile photo, which can be fictitious.",
        "NSMicrophoneUsageDescription": "This app uses the microphone if the user wants to record a video"
      }
    },
    "entryPoint": "node_modules/expo/AppEntry.js",
    "description": ""
  }
}